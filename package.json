{
  "name": "iamdeveloper-dot-com",
  "description": "My blog, based off of the Netlify CMS Gatsby Starter, https://github.com/netlify-templates/gatsby-starter-netlify-cms.",
  "version": "1.0.0",
  "author": "Nick Taylor",
  "keywords": [
    "gatsby",
    "blog",
    "react",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .cache public src/cms/*.js",
    "prebuild": "npm run clean && npm run lint && npm run type-check && npm run build-cms-admin",
    "build": "gatsby build && cp ./keybase.txt ./public/",
    "prebuild-cms-admin": "npm run clean && cp -R node_modules/gatsby/cache-dir/ .cache/",
    "build-cms-admin": "webpack --config webpack.config.cms.ts",
    "postbuild-cms-admin": "rimraf .cache",
    "predevelop": "npm run clean && npm run build-cms-admin",
    "develop": "gatsby develop",
    "type-check": "tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "lint:js": "eslint . --ext .ts,.tsx,.js",
    "lint:css": "stylelint src/**/*.scss --config .styletinrc.json",
    "lint": "npm run lint:css && npm run lint:js",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.{sass,scss,css}": [
      "stylelint --config .styletinrc.json",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80,
    "tabWidth": 4
  },
  "dependencies": {
    "@raae/gatsby-remark-oembed": "0.0.6",
    "bulma": "0.7.4",
    "gatsby": "2.3.27",
    "gatsby-image": "2.0.39",
    "gatsby-plugin-feed": "2.1.1",
    "gatsby-plugin-google-analytics": "2.0.18",
    "gatsby-plugin-manifest": "2.0.29",
    "gatsby-plugin-netlify": "2.0.15",
    "gatsby-plugin-netlify-cms": "3.0.18",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-purgecss": "3.1.1",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sass": "2.0.11",
    "gatsby-plugin-sharp": "2.0.35",
    "gatsby-plugin-sitemap": "2.0.12",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-remark-copy-linked-files": "2.0.11",
    "gatsby-remark-images": "3.0.11",
    "gatsby-remark-prismjs": "3.2.8",
    "gatsby-remark-relative-images": "0.2.2",
    "gatsby-source-filesystem": "2.0.31",
    "gatsby-transformer-remark": "2.3.8",
    "gatsby-transformer-sharp": "2.1.18",
    "lodash": "4.17.11",
    "lodash-webpack-plugin": "0.11.5",
    "netlify-cms": "2.9.1",
    "prismjs": "1.16.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-social-sharing": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/lodash": "4.14.123",
    "@types/react": "16.8.14",
    "@types/react-helmet": "5.0.8",
    "@types/webpack": "4.4.27",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "gatsby-plugin-typescript": "2.0.13",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "node-glob": "1.2.0",
    "node-sass": "4.11.0",
    "postcss-pxtorem": "4.0.1",
    "prettier": "1.17.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-scss": "3.6.0",
    "ts-node": "8.1.0",
    "typescript": "3.4.5",
    "typescript-plugin-css-modules": "1.0.5",
    "webpack-cli": "3.3.1"
  }
}
