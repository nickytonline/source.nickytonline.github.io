{
  "name": "iamdeveloper-dot-com",
  "description": "My blog, based off of the Netlify CMS Gatsby Starter, https://github.com/netlify-templates/gatsby-starter-netlify-cms.",
  "version": "1.0.0",
  "author": "Nick Taylor",
  "keywords": [
    "gatsby",
    "blog",
    "react"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run develop",
    "clean": "rimraf .cache public",
    "build": "npm run clean && gatsby build && cp ./keybase.txt ./public/",
    "develop": "npm run clean && gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "gatsby": "^2.0.89",
    "gatsby-image": "^2.0.25",
    "gatsby-plugin-feed": "^2.0.11",
    "gatsby-plugin-google-analytics": "^2.0.9",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cms": "^3.0.9",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-purgecss": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-sitemap": "^2.0.4",
    "gatsby-plugin-twitter": "^2.0.8",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-linked-files": "^2.0.8",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.13",
    "gatsby-transformer-remark": "^2.2.0",
    "gatsby-transformer-sharp": "^2.1.10",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms": "^2.3.2",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/webpack": "^4.4.22",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "gatsby-plugin-typescript": "^2.0.3",
    "gh-pages": "^2.0.1",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "lost": "^8.3.0",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.15.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  }
}
